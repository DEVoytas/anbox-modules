PACKAGE_NAME="anbox"
PACKAGE_VERSION="1"
CLEAN="make -C common clean && make -C ashmem clean && make -C binder clean"
MAKE[0]="'make' -j$parallel_jobs -C common KERNEL_SRC=$kernel_source_dir && 'make' -j$parallel_jobs -C ashmem KERNEL_SRC=$kernel_source_dir && make -j$parallel_jobs -C binder KERNEL_SRC=$kernel_source_dir"
BUILT_MODULE_NAME[0]="anbox_common_linux"
BUILT_MODULE_LOCATION[0]="common"
DEST_MODULE_LOCATION[0]="/updates"
BUILT_MODULE_NAME[1]="ashmem_linux"
BUILT_MODULE_LOCATION[1]="ashmem"
DEST_MODULE_LOCATION[1]="/updates"
BUILT_MODULE_NAME[2]="binder_linux"
BUILT_MODULE_LOCATION[2]="binder"
DEST_MODULE_LOCATION[2]="/updates"
AUTOINSTALL="yes"
